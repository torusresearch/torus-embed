  "peerDependencies": {
    "@babel/runtime": "7.x"
  },
  "dependencies": {
    "@toruslabs/http-helpers": "^4.0.0",
    "@toruslabs/openlogin-jrpc": "^4.7.0",
    "@metamask/rpc-errors": "^5.1.1",
    "events": "^3.3.0",
    "fast-deep-equal": "^3.1.3",
    "is-stream": "^2.0.1",
    "lodash.merge": "^4.6.2",
    "loglevel": "^1.8.1",
    "pump": "^3.0.0",
    "readable-stream": "^4.4.1"
  },
  "devDependencies": {
    "@babel/runtime": "^7.22.5",
    "@rollup/plugin-replace": "^5.0.2",
    "@toruslabs/eslint-config-typescript": "^2.0.0",
    "@toruslabs/torus-scripts": "^4.0.0",
    "@types/create-hash": "^1.2.2",
    "@types/lodash.merge": "^4.6.7",
    "@types/node": "^18",
    "@types/readable-stream": "^2.3.15",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.43.0",
    "eslint-plugin-import": "~2.27.5",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "mocha": "^10.2.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "mocha": {
    "timeout": 0
  },
  "keywords": [
    "torus",
    "wallet",
    "ERC20",
    "Ethereum",
    "embed",
    "login",
    "OAuth",
    "crypto"
  ],
  "author": "Torus Labs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/torusresearch/torus-embed/issues"
  },
  "homepage": "https://github.com/torusresearch/torus-embed#readme",
  "lint-staged": {
    "!(*d).{js,ts}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{json, md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=16.18.1",
    "npm": ">=8.x"
  }
}
